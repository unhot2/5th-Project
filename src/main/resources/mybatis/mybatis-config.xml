<?xml version="1.0" encoding="UTF-8"?>

	<!DOCTYPE configuration
	    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	    "http://mybatis.org/dtd/mybatis-3-config.dtd">
	<configuration>
		<typeAliases>
		<!--  alias 는 리턴타입의 별명, type은 리턴값을 TestDTO 타입으로 받아온다는 의미 -->
			<typeAlias alias="loginDTO" type="com.team.dto.LoginDTO"/>
			<typeAlias alias="boardDTO" type="com.team.dto.BoardDTO"/>
			<typeAlias alias="noticeDTO" type="com.team.dto.NoticeDTO"/>
		</typeAliases>
	</configuration>